{
  "name": "webdrioTests",
  "version": "1.0.0",
  "description": "Boilerplate project to run WebdriverIO tests with Appium to test native applications on iOS and Android",
  "main": "wdio.conf.js",
  "scripts": {
    "android.app": "wdio ./config/wdio.android.app.conf.js",
    "android.browser": "wdio ./config/wdio.android.browser.conf.js",
    "ios.app": "wdio ./config/wdio.ios.app.conf.js",
    "ios.browser": "wdio ./config/wdio.ios.browser.conf.js",
    "web": "wdio config/wdio.web.browser.conf.js",
    "web.suite1": "wdio config/wdio.web.browser.conf.js --suite demos",
    "hybridTests": "npm i && npm run web.browser && npm run ios.app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio/appium-boilerplate.git"
  },
  "keywords": [
    "WebdriverIO",
    "WebDriver",
    "appium",
    "native app",
    "hybrid",
    "android",
    "ios"
  ],
  "author": "Wim Selles <wim@wswebcreation.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/appium-boilerplate/issues"
  },
  "homepage": "https://github.com/webdriverio/appium-boilerplate#readme",
  "dependencies": {
    "moment": "2.24.0"
  },
  "devDependencies": {
    "@wdio/cli": "^5.16.11",
    "@wdio/jasmine-framework": "^5.16.11",
    "appium": "^1.4",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chromedriver": "77.0.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "geckodriver": "^1.19.1",
    "wdio-appium-service": "^0.2.3",
    "wdio-devtools-service": "^0.1.6",
    "wdio-jasmine-framework": "^0.3.6",
    "wdio-selenium-standalone-service": "0.0.12",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.14.4"
  }
}
